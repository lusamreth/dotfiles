!rofi.hlbg:                           #34405a
!rofi.bc:                             #789ee1
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #c5c8c6,  #c5c8c6,  #353944,  #c5c8c6,  #FFFFFF
!rofi.color-urgent: #002b36
!rofi.color-active: #002b36
rofi.font:       Iosevka 16
rofi.color-window: #353944,      #404552

! special
*.foreground:   #c5c8c6
*.background:   #1e1f29
*.cursorColor:  #c5c8c6

