set number
set relativenumber
set autoindent
set tabstop=4
set softtabstop=4
set expandtab

set showcmd
set encoding=utf-8
set laststatus=2
set autoindent 
set smartindent

set matchpairs+=<:>

syntax on
colorscheme monokai

map <C-j> <C-W>j
map <C-k> <C-W>k
map <C-h> <C-W>h
map <C-l> <C-W>l
inoremap jk <esc>


















